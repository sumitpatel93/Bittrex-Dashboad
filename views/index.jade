doctype html
html
  head
    title= title
    link(rel='stylesheet', href='/stylesheets/style.css')

block content
  h1 Welcome to  #{title}
  a(href="users")
   button(type="button") USERS

  h2 Market Name:  #{bittrexData.MarketName}
  p High  #{bittrexData.High}  
  p Low  #{bittrexData.Low}
  p Bid #{bittrexData.Bid}
  p OpenBuyOrders #{bittrexData.OpenBuyOrders}
